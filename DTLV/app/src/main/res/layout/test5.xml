<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/blueBackground">

    <LinearLayout
        style="@style/blueBackground"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="1" >

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_weight="0.03"
            android:id="@+id/frameLayout7" >

            <FrameLayout
                android:layout_width="8dp"
                android:layout_height="match_parent"
                android:layout_gravity="left|center_vertical" />

            <TextView
                android:layout_width="43dp"
                android:layout_height="match_parent"
                android:id="@+id/textView14"
                android:layout_gravity="center"
                android:background="#ffffff" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:id="@+id/frameLayout8"
            android:layout_weight="0.04" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="483dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:id="@+id/linearLayout8"
            android:weightSum="1"
            android:layout_weight="0.87" >

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:weightSum="1"
                android:id="@+id/linearLayout7" >

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_weight="0.05"
                    android:id="@+id/linearLayout6"
                    android:weightSum="1" >

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="135dp"
                        android:layout_weight="0.98"
                        android:id="@+id/linearLayout5"
                        android:weightSum="1" >

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.31"
                            android:id="@+id/frameLayout9" />

                        <TextView
                            android:layout_width="767dp"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:text="@string/test5_name"
                            android:id="@+id/textView17"
                            android:background="#ffffff"
                            android:layout_weight="0.19" />

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_weight="0.15" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/test5_instruction"
                            android:id="@+id/textView20"
                            android:background="#ffffff"
                            android:layout_weight="0.19" />
                    </LinearLayout>

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="0.05"
                        android:id="@+id/frameLayout10" >

                        <ImageButton
                            android:layout_width="110dp"
                            android:layout_height="100dp"
                            android:id="@+id/test5_bhelp"
                            android:layout_gravity="center"
                            android:background="@drawable/help_3"
                            android:clickable="true" />
                    </FrameLayout>
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="@string/test5_text1_part0"
                                android:id="@+id/textView1"
                                android:background="#404a4444" />

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="1030dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:weightSum="1">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical">

                                    <TextView
                                        android:layout_width="200dp"
                                        android:layout_height="200dp"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:clickable="true"
                                        android:enabled="true"
                                        android:id="@+id/test5_text1"
                                        android:text="@string/test5_text1_part1"
                                        android:layout_gravity="center_horizontal"
                                        android:textSize="25dp"/>

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center_vertical"
                                        android:weightSum="1">

                                        <FrameLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.28"></FrameLayout>

                                        <ImageButton
                                            android:layout_width="60dp"
                                            android:layout_height="60dp"
                                            android:scaleType="fitXY"
                                            android:id="@+id/test5_validate1"
                                            android:src="@drawable/validate"
                                            android:background="@null"
                                            android:layout_gravity="center" />

                                        <FrameLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.26"></FrameLayout>

                                        <ImageButton
                                            android:layout_width="60dp"
                                            android:layout_height="60dp"
                                            android:scaleType="fitXY"
                                            android:id="@+id/test5_refuse1"
                                            android:src="@drawable/refuse"
                                            android:background="@null"
                                            android:layout_gravity="center" />

                                    </LinearLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical" >

                                    <FrameLayout
                                        android:layout_width="62dp"
                                        android:layout_height="match_parent"></FrameLayout>

                                    <TextView
                                        android:layout_width="200dp"
                                        android:layout_height="200dp"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:clickable="true"
                                        android:enabled="true"
                                        android:id="@+id/test5_text2"
                                        android:text="@string/test5_text1_part2"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="25dp"/>

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center_vertical"
                                        android:weightSum="1" >

                                        <FrameLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.28" />

                                        <ImageButton
                                            android:layout_width="60dp"
                                            android:layout_height="60dp"
                                            android:scaleType="fitXY"
                                            android:id="@+id/test5_validate2"
                                            android:src="@drawable/validate"
                                            android:background="@null" />

                                        <FrameLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.26" />

                                        <ImageButton
                                            android:layout_width="60dp"
                                            android:layout_height="60dp"
                                            android:scaleType="fitXY"
                                            android:id="@+id/test5_refuse2"
                                            android:src="@drawable/refuse"
                                            android:background="@null" />

                                    </LinearLayout>
                                </LinearLayout>

                                <FrameLayout
                                    android:layout_width="48dp"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical" />

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical" >

                                    <TextView
                                        android:layout_width="200dp"
                                        android:layout_height="200dp"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:clickable="true"
                                        android:enabled="true"
                                        android:id="@+id/test5_text3"
                                        android:text="@string/test5_text1_part3"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="25dp"/>

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center_vertical"
                                        android:weightSum="1" >

                                        <FrameLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.28" />

                                        <ImageButton
                                            android:layout_width="60dp"
                                            android:layout_height="60dp"
                                            android:scaleType="fitXY"
                                            android:id="@+id/test5_validate3"
                                            android:src="@drawable/validate"
                                            android:background="@null" />

                                        <FrameLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.26" />

                                        <ImageButton
                                            android:layout_width="60dp"
                                            android:layout_height="60dp"
                                            android:scaleType="fitXY"
                                            android:id="@+id/test5_refuse3"
                                            android:src="@drawable/refuse"
                                            android:background="@null" />

                                    </LinearLayout>

                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="@string/test5_text2_part0"
                                android:id="@+id/textView34"
                                android:background="#404a4444" />

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_weight="0.25">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:weightSum="1" >

                                    <FrameLayout
                                        android:layout_width="56dp"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center_vertical" />

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center_vertical" >

                                        <TextView
                                            android:layout_width="200dp"
                                            android:layout_height="200dp"
                                            android:textAppearance="?android:attr/textAppearanceLarge"
                                            android:clickable="true"
                                            android:enabled="true"
                                            android:id="@+id/test5_text4"
                                            android:text="@string/test5_text2_part1"
                                            android:layout_gravity="center_vertical"
                                            android:textSize="25dp"/>

                                        <LinearLayout
                                            android:orientation="vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_gravity="center_vertical"
                                            android:weightSum="1" >

                                            <FrameLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="0.28" />

                                            <ImageButton
                                                android:layout_width="60dp"
                                                android:layout_height="60dp"
                                                android:scaleType="fitXY"
                                                android:id="@+id/test5_validate4"
                                                android:src="@drawable/validate"
                                                android:background="@null" />

                                            <FrameLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="0.26" />

                                            <ImageButton
                                                android:layout_width="60dp"
                                                android:layout_height="60dp"
                                                android:scaleType="fitXY"
                                                android:id="@+id/test5_refuse4"
                                                android:src="@drawable/refuse"
                                                android:background="@null" />

                                        </LinearLayout>
                                    </LinearLayout>

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center_vertical" >

                                        <FrameLayout
                                            android:layout_width="63dp"
                                            android:layout_height="match_parent" />

                                        <TextView
                                            android:layout_width="200dp"
                                            android:layout_height="200dp"
                                            android:textAppearance="?android:attr/textAppearanceLarge"
                                            android:clickable="true"
                                            android:enabled="true"
                                            android:id="@+id/test5_text5"
                                            android:text="@string/test5_text2_part2"
                                            android:layout_gravity="center_vertical"
                                            android:textSize="25dp"/>

                                        <LinearLayout
                                            android:orientation="vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_gravity="center_vertical"
                                            android:weightSum="1" >

                                            <FrameLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="0.28" />

                                            <ImageButton
                                                android:layout_width="60dp"
                                                android:layout_height="60dp"
                                                android:scaleType="fitXY"
                                                android:id="@+id/test5_validate5"
                                                android:src="@drawable/validate"
                                                android:background="@null" />

                                            <FrameLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="0.26" />

                                            <ImageButton
                                                android:layout_width="60dp"
                                                android:layout_height="60dp"
                                                android:scaleType="fitXY"
                                                android:id="@+id/test5_refuse5"
                                                android:src="@drawable/refuse"
                                                android:background="@null" />

                                        </LinearLayout>
                                    </LinearLayout>

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center_vertical" >

                                        <FrameLayout
                                            android:layout_width="46dp"
                                            android:layout_height="match_parent"
                                            android:layout_gravity="center_vertical" />

                                        <TextView
                                            android:layout_width="200dp"
                                            android:layout_height="200dp"
                                            android:textAppearance="?android:attr/textAppearanceLarge"
                                            android:clickable="true"
                                            android:enabled="true"
                                            android:id="@+id/test5_text6"
                                            android:text="@string/test5_text2_part3"
                                            android:layout_gravity="center_vertical"
                                            android:textSize="25dp"/>

                                        <LinearLayout
                                            android:orientation="vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_gravity="center_vertical"
                                            android:weightSum="1" >

                                            <FrameLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="0.28" />

                                            <ImageButton
                                                android:layout_width="60dp"
                                                android:layout_height="60dp"
                                                android:scaleType="fitXY"
                                                android:id="@+id/test5_validate6"
                                                android:src="@drawable/validate"
                                                android:background="@null" />

                                            <FrameLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="0.26" />

                                            <ImageButton
                                                android:layout_width="60dp"
                                                android:layout_height="60dp"
                                                android:scaleType="fitXY"
                                                android:id="@+id/test5_refuse6"
                                                android:src="@drawable/refuse"
                                                android:background="@null" />

                                        </LinearLayout>
                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="69dp"
                        android:layout_gravity="center_horizontal"
                        android:weightSum="1" >

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="0.98" />

                        <ImageButton
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:id="@+id/test5_bnext"
                            android:src="@drawable/next"
                            android:layout_gravity="bottom"
                            android:layout_alignParentRight="true"
                            android:layout_marginLeft="5dp"
                            android:layout_marginBottom="5dp"
                            android:background="@null" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>