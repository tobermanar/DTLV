<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    style="@style/blueBackground"
    android:weightSum="1"
    android:baselineAligned="false">


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center">

        <FrameLayout
            android:layout_width="83dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:id="@+id/frameLayout27" >

            <FrameLayout
                android:layout_width="8dp"
                android:layout_height="match_parent"
                android:layout_gravity="left|center_vertical" />

            <TextView
                android:layout_width="43dp"
                android:layout_height="match_parent"
                android:id="@+id/textView6"
                android:layout_gravity="center"
                android:background="#ffffff" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:id="@+id/frameLayout25"
            android:layout_weight="0.04" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:weightSum="1" >

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.08" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:weightSum="1" >

                <TextView
                    android:layout_width="443dp"
                    android:layout_height="38dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/config_name"
                    android:id="@+id/textView29"
                    android:background="#ffffff" />

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="0.89" />

                <ImageButton
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:id="@+id/imageButton"
                    android:background="@drawable/help_3"
                    android:clickable="true" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.24" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="804dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="#ffffff"
                android:weightSum="1"
                android:layout_weight="0.20">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_weight="0.37"
                    android:weightSum="1" >

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="0.10" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="LuminositÃ© : "
                        android:id="@+id/textView37"
                        android:layout_weight="0.05" />

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Exemple d&apos;image"
                        android:id="@+id/config_image_lumino"
                        android:layout_weight="0.76" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="525dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar"
                    android:layout_gravity="center"
                    android:progress="50"
                    android:max="100" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="0.10" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="813dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="0.23"
                android:background="#ffffff"
                android:weightSum="1" >

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="26dp"
                    android:weightSum="1"
                    android:layout_weight="0.47" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Volume sonore : "
                        android:id="@+id/textView38"
                        android:layout_weight="0.07" />

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Exemple sonore"
                        android:id="@+id/config_son_volume"
                        android:layout_weight="0.83" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="525dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar2"
                    android:indeterminate="false"
                    android:progress="50"
                    android:max="100"
                    android:layout_gravity="center" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="1198dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="0.29" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="0.20"
                android:weightSum="1" >

                <FrameLayout
                    android:layout_width="130dp"
                    android:layout_height="105dp"
                    android:layout_weight="0.97" />

                <ImageButton
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:id="@+id/imageButton2"
                    android:background="@drawable/next"
                    android:adjustViewBounds="true"
                    android:cropToPadding="true"
                    android:clickable="true" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>